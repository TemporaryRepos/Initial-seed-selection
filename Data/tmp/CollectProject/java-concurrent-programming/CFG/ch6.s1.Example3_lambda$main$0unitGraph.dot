digraph "unitGraph" {
    "i0 := @parameter0: int"
    "$i1 = i0 % 2"
    "if $i1 != 0 goto $i2 = i0 + 1"
    "$i2 = i0"
    "goto [?= return $i2]"
    "$i2 = i0 + 1"
    "return $i2"
    "i0 := @parameter0: int"->"$i1 = i0 % 2";
    "$i1 = i0 % 2"->"if $i1 != 0 goto $i2 = i0 + 1";
    "if $i1 != 0 goto $i2 = i0 + 1"->"$i2 = i0";
    "if $i1 != 0 goto $i2 = i0 + 1"->"$i2 = i0 + 1";
    "$i2 = i0"->"goto [?= return $i2]";
    "goto [?= return $i2]"->"return $i2";
    "$i2 = i0 + 1"->"return $i2";
}
