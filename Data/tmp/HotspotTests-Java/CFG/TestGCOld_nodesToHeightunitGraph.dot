digraph "unitGraph" {
    "l1 := @parameter0: long"
    "i5 = 1"
    "l6 = 1L"
    "goto [?= $l0 = l6 + l6]"
    "l6 = l6 + l6"
    "i5 = i5 + 1"
    "$l0 = l6 + l6"
    "$l2 = $l0 - 1L"
    "$b3 = $l2 cmp l1"
    "if $b3 <= 0 goto l6 = l6 + l6"
    "$i4 = i5 - 1"
    "return $i4"
    "l1 := @parameter0: long"->"i5 = 1";
    "i5 = 1"->"l6 = 1L";
    "l6 = 1L"->"goto [?= $l0 = l6 + l6]";
    "goto [?= $l0 = l6 + l6]"->"$l0 = l6 + l6";
    "l6 = l6 + l6"->"i5 = i5 + 1";
    "i5 = i5 + 1"->"$l0 = l6 + l6";
    "$l0 = l6 + l6"->"$l2 = $l0 - 1L";
    "$l2 = $l0 - 1L"->"$b3 = $l2 cmp l1";
    "$b3 = $l2 cmp l1"->"if $b3 <= 0 goto l6 = l6 + l6";
    "if $b3 <= 0 goto l6 = l6 + l6"->"$i4 = i5 - 1";
    "if $b3 <= 0 goto l6 = l6 + l6"->"l6 = l6 + l6";
    "$i4 = i5 - 1"->"return $i4";
}
