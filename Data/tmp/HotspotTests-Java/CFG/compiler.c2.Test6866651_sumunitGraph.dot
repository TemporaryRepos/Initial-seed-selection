digraph "unitGraph" {
    "i0 = 0"
    "i1 = 1"
    "i2 = 0"
    "goto [?= (branch)]"
    "i0 = i0 ^ i2"
    "i1 = i1 + 1"
    "i2 = i2 + -1"
    "if i1 != 0 goto i0 = i0 ^ i2"
    "return i0"
    "i0 = 0"->"i1 = 1";
    "i1 = 1"->"i2 = 0";
    "i2 = 0"->"goto [?= (branch)]";
    "goto [?= (branch)]"->"if i1 != 0 goto i0 = i0 ^ i2";
    "i0 = i0 ^ i2"->"i1 = i1 + 1";
    "i1 = i1 + 1"->"i2 = i2 + -1";
    "i2 = i2 + -1"->"if i1 != 0 goto i0 = i0 ^ i2";
    "if i1 != 0 goto i0 = i0 ^ i2"->"return i0";
    "if i1 != 0 goto i0 = i0 ^ i2"->"i0 = i0 ^ i2";
}
