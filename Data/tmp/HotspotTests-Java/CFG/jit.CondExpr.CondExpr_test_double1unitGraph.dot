digraph "unitGraph" {
    "d0 := @parameter0: double"
    "$b0 = d0 cmpl 10.0"
    "if $b0 != 0 goto $d1 = 2.0"
    "$d1 = 1.0"
    "goto [?= return $d1]"
    "$d1 = 2.0"
    "return $d1"
    "d0 := @parameter0: double"->"$b0 = d0 cmpl 10.0";
    "$b0 = d0 cmpl 10.0"->"if $b0 != 0 goto $d1 = 2.0";
    "if $b0 != 0 goto $d1 = 2.0"->"$d1 = 1.0";
    "if $b0 != 0 goto $d1 = 2.0"->"$d1 = 2.0";
    "$d1 = 1.0"->"goto [?= return $d1]";
    "goto [?= return $d1]"->"return $d1";
    "$d1 = 2.0"->"return $d1";
}
