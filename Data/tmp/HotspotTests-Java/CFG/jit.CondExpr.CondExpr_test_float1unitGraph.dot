digraph "unitGraph" {
    "f0 := @parameter0: float"
    "$b0 = f0 cmpl 10.0F"
    "if $b0 != 0 goto $f1 = 2.0F"
    "$f1 = 1.0F"
    "goto [?= return $f1]"
    "$f1 = 2.0F"
    "return $f1"
    "f0 := @parameter0: float"->"$b0 = f0 cmpl 10.0F";
    "$b0 = f0 cmpl 10.0F"->"if $b0 != 0 goto $f1 = 2.0F";
    "if $b0 != 0 goto $f1 = 2.0F"->"$f1 = 1.0F";
    "if $b0 != 0 goto $f1 = 2.0F"->"$f1 = 2.0F";
    "$f1 = 1.0F"->"goto [?= return $f1]";
    "goto [?= return $f1]"->"return $f1";
    "$f1 = 2.0F"->"return $f1";
}
