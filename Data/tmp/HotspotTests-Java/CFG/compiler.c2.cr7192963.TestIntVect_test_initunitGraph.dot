    "r0 := @parameter0: int[]"->"i2 = 0";
    "i2 = 0"->"goto [?= $i0 = lengthof r0]";
    "goto [?= $i0 = lengthof r0]"->"$i0 = lengthof r0";
    "$i1 = i2 & 3"->"r0[i2] = $i1";
    "r0[i2] = $i1"->"i2 = i2 + 1";
    "i2 = i2 + 1"->"$i0 = lengthof r0";
    "$i0 = lengthof r0"->"if i2 < $i0 goto $i1 = i2 & 3";
    "if i2 < $i0 goto $i1 = i2 & 3"->"return";
    "if i2 < $i0 goto $i1 = i2 & 3"->"$i1 = i2 & 3";
